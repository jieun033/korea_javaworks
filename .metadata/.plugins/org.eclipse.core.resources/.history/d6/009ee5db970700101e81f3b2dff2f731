package types;

public class CharType {

	public static void main(String[] args) {
		System.out.println("***** 문자 자료형 *****");
		
		char ch1 = 'A';
		System.out.println(ch1);
		System.out.println((int)ch1); // 아스키 코드값
	}

}       System.out.println(ch1);
        System.out.println((int)ch1); //아스키 코드값 (int)-명사적 형변환
        
