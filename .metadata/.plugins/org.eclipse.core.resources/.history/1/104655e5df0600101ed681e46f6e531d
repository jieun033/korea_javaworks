package types;

public class NumberType {

	public static void main(String[] args) {
		// 숫자 자료형
		System.out.println("***** 정수 자료형 *****");
		
		byte bData1 = 127;
		// byte bData2 = 128; 범위 초과로 에러
		
		System.out.println(bData1);
		
		long iNum1 = 220000000999999l;
		
		System.out.println(iNum1);
	}

}
